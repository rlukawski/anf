{
  "name": "anf-webapp",
  "private": true,
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "test:unit": "cross-env CI=true react-app-rewired test",
    "check:circular-deps": "madge --warning --circular --extensions ts,tsx src",
    "dump:deps": "madge --json --include-npm src/index.tsx > deps.json",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@axe-core/react": "^4.4.3",
    "@mdx-js/react": "^2.3.0",
    "@storybook/addon-a11y": "^7.0.20",
    "@storybook/addon-actions": "^7.0.20",
    "@storybook/addon-essentials": "^7.0.20",
    "@storybook/addon-interactions": "^7.0.20",
    "@storybook/addon-links": "^7.0.20",
    "@storybook/blocks": "^7.0.20",
    "@storybook/mdx1-csf": "^1.0.0",
    "@storybook/preset-create-react-app": "^7.0.20",
    "@storybook/react": "^7.0.20",
    "@storybook/react-webpack5": "^7.0.20",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^27.5.2",
    "@types/jest-axe": "^3.5.3",
    "@types/node": "^16.11.39",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@types/react-modal": "^3.13.1",
    "@types/simplemde": "^1.11.8",
    "@types/styled-components": "^5.1.26",
    "@types/uuid": "^8.3.4",
    "axe-core": "^4.4.2",
    "axios": "^1.4.0",
    "babel-plugin-named-exports-order": "^0.0.2",
    "classnames": "^2.3.1",
    "cross-env": "^7.0.3",
    "fetch-mock": "^9.11.0",
    "flush-promises": "^1.0.2",
    "highlight.js": "^10.7.3",
    "husky": "^4.3.8",
    "isomorphic-fetch": "^3.0.0",
    "jest": "^27.5.1",
    "jest-axe": "^6.0.0",
    "jest-styled-components": "^7.1.1",
    "madge": "^5.0.2",
    "msw": "^1.2.2",
    "node-polyfill-webpack-plugin": "^2.0.1",
    "polished": "^4.2.2",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-app-rewired": "^2.2.1",
    "react-dom": "^18.2.0",
    "react-markdown": "^8.0.3",
    "react-modal": "^3.15.1",
    "react-router": "6.3.0",
    "react-router-dom": "6.3.0",
    "react-scripts": "^5.0.1",
    "simplemde": "^1.11.2",
    "storybook": "^7.0.20",
    "styled-components": "^5.3.11",
    "ts-jest": "^27.1.5",
    "typescript": "~4.6.4",
    "uuid": "^8.3.2",
    "web-vitals": "^2.1.4",
    "webpack": "^5.73.0",
    "webpack-bundle-analyzer": "^4.5.0"
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "m2/setup",
      "m3/homework/contacts"
    ],
    "moduleNameMapper": {
      "^axios$": "axios/dist/axios.js"
    }
  },
  "madge": {
    "tsConfig": "tsconfig.json"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "engines": {
    "node": ">=16.14.0",
    "npm": ">=8.7.0"
  }
}
